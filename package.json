{
  "name": "gthubweb",
  "version": "0.1.0",
  "description": "New GTHub's website design using Node.js, Express and Nuxt.js",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server --watch api",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "npm run lint && cross-env NODE_ENV=test mocha -R spec api/test --exit",
    "test_connection": "node ./database/test_connection.js",
    "test_api": "cross-env NODE_ENV=test mocha -R spec api/test --exit",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "create_database": "npm run create_tables && npm run init_data && npm run insert_testdata",
    "create_tables": "node ./database/create_tables.js",
    "init_data": "node ./database/init.js",
    "insert_testdata": "node ./database/insert_testdata.js"
  },
  "dependencies": {
    "@nuxt/types": "^2.14.7",
    "@nuxtjs/axios": "^5.12.2",
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/supertest": "^2.0.10",
    "async": "^3.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "country-list": "^2.2.0",
    "cross-env": "^7.0.2",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ical-generator": "^1.15.2",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.1",
    "method-override": "^3.0.0",
    "nuxt": "^2.14.6",
    "sequelize": "^5.22.3",
    "supertest": "^6.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.12.13",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@types/mocha": "^8.0.4",
    "eslint": "^7.10.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "stylelint": "^13.7.2",
    "stylelint-config-standard": "^20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gthubdev/website.git"
  },
  "author": "KazuoCode, Wl-Vlaeyen, Delta-One",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/gthubdev/website/issues"
  },
  "homepage": "https://github.com/gthubdev/website#readme",
  "keywords": [
    "Javascript",
    "Node.js"
  ]
}
